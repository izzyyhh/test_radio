[
<% playlist = Server.find(params[:server_id]).playlist %>
<% playlist_ordered = playlist.tracks.order(:created_at => :asc) %>
<% playlist_ordered.each do |t| %>
    {
        "uri": "<%= t.uri %>",
        "starttime": "<%= t.starttime.to_i %>",
        "duration": "<%= t.duration %>",
        "title": "<%= t.title %>",
        "thumbnail": "<%= t.thumbnail %>",
        "channeltitle": "<%= t.channeltitle %>"
    }
    <% if t != playlist_ordered.last %>
    ,
    <% end %>
<% end %>
]